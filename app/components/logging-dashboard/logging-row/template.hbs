<tr class="main-row" style="vertical-align: text-top" >
  {{#if bulkActions}}
    <td valign="middle" class="row-check" style="padding-top: 2px;">
      {{check-box nodeId=model.id}}
    </td>
  {{/if}}

  <td class="{{tdPadding}}">
    <i
      role="button"
      {{action "toggle"}}
      class="icon icon-play eased text-small text-muted {{if expanded 'icon-rotate-90'}}"
    >
      <span class="visually-hidden">open accordion</span>
    </i>
  </td>

  <td class="{{tdPadding}}" data-title="{{dt.timestamp}}" >
    {{if expanded timestamp fromNow}}
  </td>

  <td class="{{tdPadding}}" data-title="{{dt.log}}" class="{{if expanded '' 'clip'}} pr-40">
    {{_log}}
  </td>
  <td class="{{tdPadding}}" data-title="{{dt.containerName}}" class="clip">
    {{model.containerName}}
  </td>
</tr>

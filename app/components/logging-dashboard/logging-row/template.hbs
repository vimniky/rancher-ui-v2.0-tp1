<tr class="main-row" style="vertical-align: text-top" >
  {{#if bulkActions}}
    <td valign="middle" class="row-check" style="padding-top: 2px;">
      {{check-box nodeId=model.id}}
    </td>
  {{/if}}

  <td class="{{tdPadding}}" data-title="{{dt.timestamp}}" >
    {{timestamp}}
    <div class="text-muted pt-5">{{fromNow}}</div>
  </td>

  <td class="{{tdPadding}}" data-title="{{dt.log}}" class="pr-40">
    {{_log}}
  </td>
  <td class="{{tdPadding}}" data-title="{{dt.containerName}}">
    {{model.containerName}}
  </td>
</tr>
